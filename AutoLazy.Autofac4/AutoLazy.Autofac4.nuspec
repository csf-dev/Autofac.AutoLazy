<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>AutoLazy.Autofac4</id>
    <version>0.0.0</version>
    <title>AutoLazy for Autofac 4.x</title>
    <authors>craigfowler</authors>
    <owners>craigfowler</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <projectUrl>https://github.com/csf-dev/Autofac.AutoLazy</projectUrl>
    <description>Extension for Autofac 4.x allowing dependency services (interfaces) to be automatically lazily-resolved, without needing to manually change their types to Lazy&lt;T&gt;.</description>
    <copyright>Copyright 2020 - CSF Software Limited</copyright>
    <repository url="https://github.com/csf-dev/Autofac.AutoLazy" />
    <dependencies>
      <group targetFramework=".NETFramework4.5">
        <dependency id="Autofac" version="[4.2.0,5.0.0)" exclude="Build,Analyzers" />
        <dependency id="Castle.Core" version="4.0.0" exclude="Build,Analyzers" />
        <dependency id="System.ComponentModel.TypeConverter" version="4.1.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETFramework4.6.1">
        <dependency id="Autofac" version="[4.2.0,5.0.0)" exclude="Build,Analyzers" />
        <dependency id="Castle.Core" version="4.0.0" exclude="Build,Analyzers" />
        <dependency id="System.ComponentModel.TypeConverter" version="4.1.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard1.3">
        <dependency id="Autofac" version="[4.2.0,5.0.0)" exclude="Build,Analyzers" />
        <dependency id="Castle.Core" version="4.0.0" exclude="Build,Analyzers" />
        <dependency id="System.ComponentModel.TypeConverter" version="4.1.0" exclude="Build,Analyzers" />
        <dependency id="NETStandard.Library" version="1.6.1" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Autofac" version="[4.2.0,5.0.0)" exclude="Build,Analyzers" />
        <dependency id="Castle.Core" version="4.0.0" exclude="Build,Analyzers" />
        <dependency id="System.ComponentModel.TypeConverter" version="4.1.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.1">
        <dependency id="Autofac" version="[4.2.0,5.0.0)" exclude="Build,Analyzers" />
        <dependency id="Castle.Core" version="4.0.0" exclude="Build,Analyzers" />
        <dependency id="System.ComponentModel.TypeConverter" version="4.1.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin/$configuration$/**/AutoLazy*.dll" target="lib" />
    <file src="bin/$configuration$/**/AutoLazy*.xml" target="lib" />
  </files>
</package>